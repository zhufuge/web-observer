{
  "manifest_version": 2,
  "name": "webwatch",
  "version": "1.0",
  "author": "zhufuge",
  "description": "Watch what you are browsing",
  "icons": {
    "48": "icons/watch-48.png",
    "96": "icons/watch-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ]
}
